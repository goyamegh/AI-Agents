{
  "name": "ai-agent-scripts",
  "version": "1.0.0",
  "description": "AI Agent with MCP/AG UI integration",
  "main": "src/main.ts",
  "scripts": {
    "start": "ts-node src/main.ts",
    "dev": "ts-node --watch src/main.ts",
    "start:ag-ui": "ts-node src/main-ag-ui.ts",
    "dev:ag-ui": "ts-node --watch src/main-ag-ui.ts",
    "build": "tsc",
    "clean": "find . -name '*.js' -not -path './node_modules/*' -delete",
    "opensearch:ingest": "ts-node src/opensearch-ingestion.ts",
    "opensearch:watch": "ts-node src/opensearch-ingestion.ts --watch"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.36",
    "@ag-ui/core": "^0.0.36",
    "@aws-sdk/client-bedrock-runtime": "^3.450.0",
    "@langchain/anthropic": "^0.2.0",
    "@langchain/aws": "^0.1.15",
    "@langchain/core": "^0.3.73",
    "@langchain/langgraph": "^0.4.9",
    "@langchain/langgraph-checkpoint-sqlite": "^0.2.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@opensearch-project/opensearch": "^3.5.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^18.0.0",
    "@types/uuid": "^10.0.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.7.0",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.0",
    "uuid": "^11.1.0",
    "ws": "^8.18.3",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node-fetch": "^2.6.7"
  }
}
